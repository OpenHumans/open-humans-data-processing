# Testing setup: Amazon credentials and variables, used by boto.
TEST_AWS_ACCESS_KEY_ID="test-amazon-access-key-id-here"
TEST_AWS_SECRET_ACCESS_KEY="test-amazon-secret-access-key-here"
TEST_AWS_S3_BUCKET="test-aws-bucket-name-here"

# Amazon credentials, used by boto.
AWS_ACCESS_KEY_ID="amazon-access-key-id-here"
AWS_SECRET_ACCESS_KEY="amazon-secret-access-key-here"

# Set to true for local dev.
DEBUG=True

# This is the same token as generated by generate-token.js in
# https://github.com/PersonalGenomesOrg/goviral-integration
# for the FIREBASE_TOKEN environment variable.
GO_VIRAL_MANAGEMENT_TOKEN='goviral-token-here'

# A RunKeeper access token to use for testing. You can get one in the main site
# database by running, for example:
# `User.objects.get(username='mpball').runkeeper.get_retrieval_params()`
RUNKEEPER_ACCESS_TOKEN="runkeeper-token-here"

# Test S3 URL for a Wild Life of Our Homes home_data.json file.
# Needed to run testing.
WILDLIFE_TESTFILE_HOMEDATA_URL=""
WILDLIFE_TESTFILE_BACTERIADATA_URL=""

# The PostgreSQL database URL.
# e.g. for local dev with a database 'mydb', user 'jdoe', password 'pa55wd':
DATABASE_URL="postgres://jdoe:pa55wd@127.0.0.1/mydb"

# A key used to communicate with open-humans; must be set in both sites
PRE_SHARED_KEY=""
